<?xml version='1.0'?>

<tag name="async" endtag="yes" casesensitive="yes">
	<tagformat nlbeforetag="1" nlbeforecontents="0" nlaftercontents="0" nlaftertag="1"/>

	<attributes>
		<attrib name="href" casesensitive="yes"/>
    	<attrib name="onexit" casesensitive="yes"/>
    	
		<attrib name="method" TYPE="Enumerated" casesensitive="yes">
      		<opt VALUE="post" caption="post"/>
      		<opt VALUE="get" caption="get (default)"/>
		</attrib>
    
		<attrib name="accept" casesensitive="yes"/>
    
		<attrib name="priority" TYPE="Enumerated" casesensitive="yes">
      		<opt VALUE="high" caption="high"/>
      		<opt VALUE="low" caption="low (default)"/>
		</attrib>
    
		<attrib name="sendreferer" TYPE="Enumerated" casesensitive="yes">
      		<opt VALUE="true" caption="true"/>
      		<opt VALUE="false" caption="false (default)"/>
		</attrib>
    
		<attrib name="id" casesensitive="yes"/>
    
		<attrib name="class" casesensitive="yes"/>
	</attributes>

	<descriptions>
		<desc lang="de">
		</desc>

		<desc lang="en">
		</desc>
	</descriptions>
</tag>
