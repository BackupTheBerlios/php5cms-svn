			<opt value="VB" caption="VB"/>
		</attrib>

		<attrib name="SRC" type="Relativepath"/>

		<attrib name="TYPE" type="Enumerated">
			<opt value="text/javascript" caption="text/javascript"/>
			<opt value="text/jscript" caption="text/jscript"/>
			<opt value="text/vbscript" caption="text/vbscript"/>
			<opt value="text/livescript" caption="text/livescript"/>
			<opt value="text/php" caption="text/php"/>
			<opt value="text/tcl" caption="text/tcl"/>
		</attrib>

		<attrib name="runat" type="Enumerated">
			<opt value="server" caption="server"/>
		</attrib>

		<attrib name="DEFER" type="Flag"/>
