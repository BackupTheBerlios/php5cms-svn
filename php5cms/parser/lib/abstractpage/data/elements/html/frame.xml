<?xml version='1.0'?>

<tag name="FRAME" endtag="no">
	<tagformat nlbeforetag="1" nlaftertag="1"/>

	<attributes>
		<attrib name="SRC" type="Relativepath"/>
		<attrib name="NAME"/>

		<attrib name="FRAMEBORDER" type="Enumerated">
			<opt value="1" caption="1   (Internet Explorer, Netscape, HTML 4.0) (default)"/>
			<opt value="0" caption="0   (Internet Explorer, Netscape, HTML 4.0)"/>
			<opt value="Yes" caption="Yes (Netscape)"/>
			<opt value="No" caption="No  (Netscape)"/>
		</attrib>

		<attrib name="SCROLLING" type="Enumerated">
			<opt value="Auto" caption="Auto (default)"	SELECTED/>
			<opt value="Yes" caption="Yes"/>
			<opt value="No" caption="No"/>
		</attrib>

		<attrib name="NORESIZE" type="Flag"/>
		<attrib name="MARGINWIDTH"/>
		<attrib name="MARGINHEIGHT"/>
		<attrib name="BORDERCOLOR" type="Color"/>
		<attrib name="CLASS" type="cssStyle"/>
		<attrib name="ID"/>
		<attrib name="STYLE" type="Style"/>
		<attrib name="TITLE"/>
		<attrib name="LONGDESC"/>
	</attributes>

	<descriptions>
		<desc lang="de">
		</desc>

		<desc lang="en">
		</desc>
	</descriptions>
</tag>
