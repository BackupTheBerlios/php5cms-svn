<?xml version='1.0'?>

<tag name="IFRAME" endtag="yes">
	<tagformat nlbeforetag="1" nlbeforecontents="0" nlaftercontents="0" nlaftertag="1"/>
	
	<attributes>
		<attrib name="SRC" type="Relativepath"/>
		<attrib name="NAME"/>
		<attrib name="WIDTH"/>
		<attrib name="MARGINWIDTH"/>
		<attrib name="HEIGHT"/>
		<attrib name="MARGINHEIGHT"/>
		
		<attrib name="ALIGN" type="Enumerated">
			<opt value="left" caption="left"/>
			<opt value="right" caption="right"/>
			<opt value="top" caption="top"/>
			<opt value="middle" caption="middle"/>
			<opt value="bottom" caption="bottom"/>
		</attrib>
		
		<attrib name="SCROLLING" type="Enumerated">
			<opt value="Auto" caption="Auto (default)"	SELECTED/>
			<opt value="Yes" caption="Yes"/>
			<opt value="No" caption="No"/>
		</attrib>
		
		<attrib name="FRAMEBORDER" type="Enumerated">
			<opt value="1" caption="1 (default)"	SELECTED/>
			<opt value="0" caption="0"/>
		</attrib>
		
		<attrib name="HSPACE"/>
		<attrib name="VSPACE"/>
		<attrib name="CLASS" type="cssStyle"/>
		<attrib name="ID"/>
		<attrib name="STYLE" type="Style"/>
		<attrib name="TITLE"/>
		<attrib name="LONGDESC"/>
	</attributes>

	<descriptions>
		<desc lang="de">
		</desc>

		<desc lang="en">
		</desc>
	</descriptions>
</tag>
