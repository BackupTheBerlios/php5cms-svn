<?xml version='1.0'?>

<tag name="plugin" casesensitive="yes" endtag="no">
	<tagformat nlbeforetag="1" nlbeforecontents="1" nlaftercontents="1" nlaftertag="1" indentcontents="yes"/>
  	<attributes>
    	<attrib name="type" type="ENUMERATED" casesensitive="yes">
      		<opt value="bean"/>
      		<opt value="applet"/>
    	</attrib>
    
		<attrib name="code" type="TEXT" casesensitive="yes">
		<attrib name="codebase" type="TEXT" casesensitive="yes">
		<attrib name="name" type="TEXT" casesensitive="yes">
		
		<attrib name="align" type="TEXT" casesensitive="yes">
      		<opt value="bottom"/>
      		<opt value="top"/>
      		<opt value="middle"/>
     	 	<opt value="left"/>
      		<opt value="right"/>
	  	</attrib>
    
		<attrib name="height" type="TEXT" casesensitive="yes">
		<attrib name="width" type="TEXT" casesensitive="yes">
		<attrib name="hspace" type="TEXT" casesensitive="yes">
		<attrib name="vspace" type="TEXT" casesensitive="yes">
		<attrib name="jreversion" type="TEXT" casesensitive="yes">
		<attrib name="nspluginurl" type="TEXT" casesensitive="yes">
		<attrib name="iepluginURL" type="TEXT" casesensitive="yes">
   	</attributes>

	<descriptions>
		<desc lang="de">
		</desc>

		<desc lang="en">
		</desc>
	</descriptions>
</tag>
